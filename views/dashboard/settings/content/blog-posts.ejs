<!DOCTYPE html>
<html lang="en">
<!--begin::Head-->
<%- include('../../partials/head', { title }) %>
<!--end::Head-->
<!--begin::Body-->

<body id="kt_app_body" data-kt-app-layout="light-sidebar" data-kt-app-header-fixed="true" data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true" data-kt-app-sidebar-push-header="true" data-kt-app-sidebar-push-toolbar="true" data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true" class="app-default">
  <!--begin::Theme mode setup on page load-->
  <%- include('../../partials/theme-mode.ejs') %>

  <% const browserTimezone = Intl.DateTimeFormat().resolvedOptions().timeZone; %>
  <!--end::Theme mode setup on page load-->
  <!--begin::App-->
  <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
    <!--begin::Page-->
    <div class="app-page flex-column flex-column-fluid" id="kt_app_page">
      <!--begin::Header-->
      <%- include('../../partials/header', { user }) %>
      <!--end::Header-->
      <!--begin::Wrapper-->
      <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
        <!--begin::Sidebar-->
        <%- include('../../partials/sidebar') %>
        <!--end::Sidebar-->
        <!--begin::Main-->
        <div class="app-main flex-column flex-row-fluid" id="kt_app_main">
          <!--begin::Content wrapper-->
          <div class="d-flex flex-column flex-column-fluid">
            <!--begin::Toolbar-->
            <div id="kt_app_toolbar" class="app-toolbar py-3 py-lg-6">
              <!--begin::Toolbar container-->
              <div id="kt_app_toolbar_container" class="app-container container-xxl d-flex flex-stack">
                <!--begin::Page title-->
                <div class="page-title d-flex flex-column justify-content-center flex-wrap me-3">
                  <!--begin::Title-->
                  <h1 class="page-heading d-flex text-gray-900 fw-bold fs-3 flex-column justify-content-center my-0">
                    Blog Posts
                  </h1>
                  <!--end::Title-->
                  <!--begin::Breadcrumb-->
                  <ul class="breadcrumb breadcrumb-separatorless fw-semibold fs-7 my-0 pt-1">
                    <!--begin::Item-->
                    <li class="breadcrumb-item text-muted">
                      <a href="..//dashboard" class="text-muted text-hover-primary">Home</a>
                    </li>
                    <!--end::Item-->
                    <!--begin::Item-->
                    <li class="breadcrumb-item">
                      <span class="bullet bg-gray-500 w-5px h-2px"></span>
                    </li>
                    <!--end::Item-->
                    <!--begin::Item-->
                    <li class="breadcrumb-item text-dark">Blog Posts</li>
                    <!--end::Item-->
                  </ul>
                  <!--end::Breadcrumb-->
                </div>
                <!--end::Page title-->
              </div>
              <!--end::Toolbar container-->
            </div>
            <!--end::Toolbar-->
            <!--begin::Content-->
            <div id="kt_app_content" class="app-content flex-column-fluid">
              <!--begin::Content container-->
              <div id="kt_app_content_container" class="app-container container-xxl">
                <!--begin::Card-->
                <div class="card">
                  <!--begin::Card header-->
                  <div class="card-header border-0 pt-6">
                    <!--begin::Card title-->
                    <div class="card-title">
                      <!--begin::Search-->
                      <div class="d-flex align-items-center position-relative my-1">
                        <i class="ki-duotone ki-magnifier fs-3 position-absolute ms-5">
                          <span class="path1"></span>
                          <span class="path2"></span>
                        </i>
                        <input type="text" data-kt-content-blog-posts-table-filter="search" class="form-control form-control-solid w-250px ps-12" placeholder="Search Blog Posts..." />
                      </div>
                      <!--end::Search-->
                    </div>
                    <!--begin::Card title-->
                    <!--begin::Card toolbar-->
                    <div class="card-toolbar">
                      <!--begin::Toolbar-->
                      <div class="d-flex justify-content-end" data-kt-content-blog-posts-table-toolbar="base">
                        <!--begin::Add Package-->
                        <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#kt_modal_add_content_blog_posts">
                          <i class="ki-duotone ki-plus fs-2"></i>
                          Add Blog Post
                        </button>
                        <!--end::Add Package-->
                      </div>
                      <!--end::Toolbar-->
                      <!--begin::Group actions-->
                      <div class="d-flex justify-content-end align-items-center d-none" data-kt-content-blog-posts-table-toolbar="selected">
                        <div class="fw-bold me-5">
                          <span class="me-2" data-kt-content-blog-posts-table-select="selected_count"></span>Selected
                        </div>
                        <button type="button" class="btn btn-danger" data-kt-content-blog-posts-table-select="delete_selected">
                          Delete Selected
                        </button>
                      </div>
                      <!--end::Group actions-->
                    </div>
                    <!--end::Card toolbar-->
                  </div>
                  <!--end::Card header-->
                  <!--begin::Card body-->
                  <div class="card-body pt-0">
                    <!--begin::Table-->
                    <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_content_blog_posts_table">
                      <thead>
                        <tr class="text-start text-muted fw-bold fs-7 text-uppercase gs-0">
                          <th class="w-10px pe-2">
                            <div class="form-check form-check-sm form-check-custom form-check-solid me-3">
                              <input class="form-check-input form-check-input-toggle" type="checkbox" data-kt-check="true" data-kt-check-target="#kt_content_blog_posts_table .form-check-input-toggle" value="1" />
                            </div>
                          </th>
                          <th class="min-w-125px">Banner</th>
                          <th class="min-w-125px">Title</th>
                          <th class="min-w-125px">Review</th>
                          <th class="min-w-125px">Number Of Reviews</th>
                          <th class="min-w-125px">Location</th>
                          <th class="min-w-125px">Status</th>
                          <th class="min-w-125px">Last Modified</th>
                          <th class="text-end min-w-100px">Actions</th>
                        </tr>
                      </thead>
                      <tbody class="text-gray-600 fw-semibold">
                        <% blogPosts?.forEach((blog) => { %>
                        <tr>
                          <td>
                            <div class="form-check form-check-sm form-check-custom form-check-solid">
                              <input class="form-check-input form-check-input-toggle" type="checkbox" value="<%= blog?._id %>" />
                            </div>
                          </td>
                          <td>
                            <div class="symbol symbol-40px">
                              <img src="<%= blog?.banner %>" alt="Banner" />
                            </div>
                          </td>
                          <td>
                            <a href="#" class="text-gray-800 text-hover-primary">
                              <%= blog?.title %>
                            </a>
                          </td>
                          <td>
                            <div class="rating">
                              <% for (let i = 0; i < 5; i++) { %>
                              <% if (i < blog?.rating) { %>
                              <i class="bi bi-star-fill"></i>
                              <% } else { %>
                              <i class="bi bi-star"></i>
                              <% } %>
                              <% } %>
                            </div>
                          </td>
                          <td><%= blog?.numberOfReviews.toLocaleString() %></td>
                          <td><%= blog?.location %></td>
                          <td>
                            <div class="form-check form-switch form-check-solid">
                              <label class="form-check-label">
                                <input class="form-check-input form-check-input-custom" type="checkbox" <% if (blog?.status === 'active') { %> checked <% } %> value="<%= blog?.status %>" data-kt-blog-posts-url="/api/settings/content/blog-posts/<%= blog?._id %>/status" />
                                <span class="enabled">Active</span>
                                <span class="disabled">Disabled</span>
                              </label>
                            </div>
                          </td>
                          <td><%= new Date(blog?.updatedAt).toLocaleString('en-US', {
                          year: 'numeric',
                          month: 'long',
                          day: 'numeric',
                          hour: '2-digit',
                          minute: '2-digit',
                          second: '2-digit',
                          timeZone: browserTimezone, // Browser timezone here
                          // timeZoneName: 'short'
                        }) %></td>
                          <td class="text-end">
                            <a href="#" class="btn btn-light btn-active-light-primary btn-flex btn-center btn-sm" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end">
                              Actions
                              <i class="ki-duotone ki-down fs-5 ms-1"></i></a>
                            <!--begin::Menu-->
                            <div class="menu menu-sub menu-sub-dropdown menu-column menu-rounded menu-gray-600 menu-state-bg-light-primary fw-semibold fs-7 w-125px py-4" data-kt-menu="true">
                              <!--begin::Menu item-->
                              <div class="menu-item px-3">
                                <a href="/api/settings/content/blog-posts/<%= blog?._id %>" role="button" class="menu-link px-3" data-bs-toggle="modal" data-bs-target="#kt_modal_edit_content_blog_posts" data-kt-content-blog-post-table-filter="edit_row">
                                  Edit
                                </a>
                              </div>
                              <!--end::Menu item-->
                              <!--begin::Menu item-->
                              <div class="menu-item px-3">
                                <a href="/api/settings/content/blog-posts/<%= blog?._id %>" class="menu-link px-3" data-kt-content-blog-posts-table-filter="delete_row" data-blog-id="<%= blog?._id %>">
                                  Delete
                                </a>
                              </div>
                              <!--end::Menu item-->
                            </div>
                            <!--end::Menu-->
                          </td>
                        </tr>
                        <% }); %>
                      </tbody>
                    </table>

                    <!--end::Table-->
                  </div>
                  <!--end::Card body-->
                </div>
                <!--end::Card-->
              </div>
              <!--end::Content container-->
            </div>
            <!--begin::Modals-->
            <!--begin::Modal - Add Blog Post-->
            <div class="modal fade" id="kt_modal_add_content_blog_posts" tabindex="-1" aria-hidden="true">
              <!--begin::Modal dialog-->
              <div class="modal-dialog modal-dialog-centered mw-650px">
                <!--begin::Modal content-->
                <div class="modal-content">
                  <!--begin::Modal header-->
                  <div class="modal-header" id="kt_modal_add_content_blog_posts_header">
                    <!--begin::Modal title-->
                    <h2 class="fw-bold">Add Blog Post</h2>
                    <!--end::Modal title-->
                    <!--begin::Close-->
                    <div class="btn btn-icon btn-sm btn-active-icon-primary" data-kt-add-content-blog-post-modal-action="close">
                      <i class="bi bi-x fs-2x"></i>
                    </div>
                    <!--end::Close-->
                  </div>
                  <!--end::Modal header-->
                  <!--begin::Modal body-->
                  <div class="modal-body px-5 my-7">
                    <!--begin::Form-->
                    <form id="kt_modal_add_content_blog_posts_form" class="form" action="/api/settings/content/blog-posts" method="post">
                      <!--begin::Scroll-->
                      <div class="d-flex flex-column scroll-y px-5 px-lg-10" id="kt_modal_add_content_blog_posts_scroll" data-kt-scroll="true" data-kt-scroll-activate="true" data-kt-scroll-max-height="auto" data-kt-scroll-dependencies="#kt_modal_add_content_blog_posts_header" data-kt-scroll-wrappers="#kt_modal_add_content_blog_posts_scroll" data-kt-scroll-offset="300px">
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">Thumbnail</label>
                          <!--end::Label-->
                          <!--begin::Input group-->
                          <div class="fv-row">
                            <!--begin::Preview Container-->
                            <div class="kt-file-uploader">
                              <!--begin::Message-->
                              <label class="kt-file-uploader-label d-flex flex-column flex-xl-row align-items-center align-items-xl-start text-center text-xl-start" for="kt_file_uploader_add_content_blog_posts_thumbnail">
                                <i class="ki-duotone ki-file-up fs-3x text-primary"><span class="path1"></span><span class="path2"></span>
                                </i>
                                <!--begin::Info-->
                                <div class="ms-xl-4 mt-4 mt-xl-0">
                                  <h3 class="fs-5 fw-bold text-gray-900 mb-1">
                                    Upload thumbnail
                                  </h3>
                                  <span class="fs-7 fw-semibold text-gray-500">
                                    PNG, JPG, JPEG, GIF up to 10MB
                                  </span>
                                </div>
                                <!--end::Info-->
                              </label>
                              <!--end::Message-->
                              <!--begin::Input-->
                              <input type="file" name="thumbnail" id="kt_file_uploader_add_content_blog_posts_thumbnail" accept=".png, .jpg, .jpeg" data-kt-file-uploader-max-size="10" hidden class="d-none" />
                              <!--begin::Error Message -->
                              <div class="invalid-feedback d-block"></div>
                              <!--end::Error Message-->
                            </div>
                            <!--end::Preview Container-->
                          </div>
                          <!--end::Input group-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">Banner</label>
                          <!--end::Label-->
                          <!--begin::Input group-->
                          <div class="fv-row">
                            <!--begin::Preview Container-->
                            <div class="kt-file-uploader">
                              <!--begin::Message-->
                              <label class="kt-file-uploader-label d-flex flex-column flex-xl-row align-items-center align-items-xl-start text-center text-xl-start" for="kt_file_uploader_add_content_blog_posts_banner">
                                <i class="ki-duotone ki-file-up fs-3x text-primary"><span class="path1"></span><span class="path2"></span></i>
                                <!--begin::Info-->
                                <div class="ms-xl-4 mt-4 mt-xl-0">
                                  <h3 class="fs-5 fw-bold text-gray-900 mb-1">Upload banner</h3>
                                  <span class="fs-7 fw-semibold text-gray-500">PNG, JPG, JPEG, GIF up to 10MB</span>
                                </div>
                                <!--end::Info-->
                              </label>
                              <!--end::Message-->
                              <!--begin::Input-->
                              <input type="file" name="banner" id="kt_file_uploader_add_content_blog_posts_banner" accept=".png, .jpg, .jpeg" data-kt-file-uploader-max-size="10" hidden class="d-none" />
                              <!--begin::Error Message-->
                              <div class="invalid-feedback d-block"></div>
                              <!--end::Error Message-->
                            </div>
                            <!--end::Preview Container-->
                          </div>
                          <!--end::Input group-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">Title</label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <input type="text" class="form-control form-control-lg form-control-solid" name="title" value="" placeholder="" />
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">Subtitle</label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <input type="text" class="form-control form-control-lg form-control-solid" name="subtitle" value="" placeholder="" />
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="fs-5 fw-semibold form-label mb-5 required">
                            Rating:
                          </label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <select data-control="select2" data-placeholder="Select a rating" data-hide-search="true" name="rating" class="form-select form-select-solid">
                            <option></option>
                            <option value="1">1 Star</option>
                            <option value="2">2 Star</option>
                            <option value="3">3 Star</option>
                            <option value="4">4 Star</option>
                            <option value="5">5 Star</option>
                          </select>
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">
                            Number Of Reviews
                          </label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <input type="number" class="form-control form-control-lg form-control-solid" name="numberOfReviews" value="" placeholder="" />
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->



                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">Description</label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <textarea class="form-control form-control-solid" name="description" rows="3" placeholder="Write a description"></textarea>
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">
                            Location
                          </label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <input type="text" class="form-control form-control-lg form-control-solid" name="location" value="" placeholder="" />
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">Status</label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <select name="status" aria-label="Select a status" data-control="select2" data-hide-search="true" data-placeholder="Select a status.." class="form-select form-select-solid form-select-lg">
                            <option value="">Select a status..</option>
                            <option value="active">Active</option>
                            <option value="disabled">Disabled</option>
                          </select>
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">
                            Nearest Airport
                          </label>
                          <!--end::Label-->
                          <!--begin::Input-->
                          <select name="nearestAirport" aria-label="Select an airport" data-control="select2" data-placeholder="Select an airport.." class="form-select form-select-solid form-select-lg">
                            <option value="">Select an airport..</option>
                            <option value="DAC">
                              Hazrat Shahjalal Intl Arpt
                            </option>
                            <option value="CGP">Shah Amanat Intl Arpt</option>
                            <option value="Sylhet">Osmani Intl Arpt</option>
                            <option value="RJH">
                              Shah Makhdum Intl Arpt
                            </option>
                            <option value="ZYL">Osmani Intl Arpt</option>
                            <option value="CXB">Cox's Bazar Intl Arpt</option>
                          </select>
                          <!--end::Input-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="fs-5 fw-semibold form-label mb-5 required">
                            Domestic Airlines
                          </label>
                          <!--end::Label-->
                          <!--begin::Options-->
                          <div class="d-flex flex-wrap gap-4">
                            <div class="form-check form-check-custom form-check-solid">
                              <input name="domesticAirlines" class="form-check-input" type="checkbox" value="Biman Bangladesh Airlines" id="kt_add_content_blog_post_domestic_airlines_1" />
                              <label class="form-check-label" for="kt_add_content_blog_post_domestic_airlines_1">
                                Biman Bangladesh Airlines
                              </label>
                            </div>
                            <div class="form-check form-check-custom form-check-solid">
                              <input name="domesticAirlines" class="form-check-input" type="checkbox" value="Novoair" id="kt_add_content_blog_post_domestic_airlines_2" />
                              <label class="form-check-label" for="kt_add_content_blog_post_domestic_airlines_2">
                                Novoair
                              </label>
                            </div>
                            <div class="form-check form-check-custom form-check-solid">
                              <input name="domesticAirlines" class="form-check-input" type="checkbox" value="US-Bangla Airlines" id="kt_add_content_blog_post_domestic_airlines_3" />
                              <label class="form-check-label" for="kt_add_content_blog_post_domestic_airlines_3">
                                US-Bangla Airlines
                              </label>
                            </div>
                            <div class="form-check form-check-custom form-check-solid">
                              <input name="domesticAirlines" class="form-check-input" type="checkbox" value="Air Astra" id="kt_add_content_blog_post_domestic_airlines_4" />
                              <label class="form-check-label" for="kt_add_content_blog_post_domestic_airlines_4">
                                Air Astra
                              </label>
                            </div>
                          </div>
                        </div>
                        <!--end::Input group-->
                      </div>
                      <!--end::Scroll-->
                      <!--begin::Actions-->
                      <div class="text-center pt-10">
                        <button type="reset" class="btn btn-light me-3" data-kt-add-content-blog-post-modal-action="cancel">
                          Discard
                        </button>
                        <button type="submit" class="btn btn-primary" data-kt-add-content-blog-post-modal-action="submit">
                          <span class="indicator-label">Submit</span>
                          <span class="indicator-progress">Please wait...
                            <span class="spinner-border spinner-border-sm align-middle ms-2"></span></span>
                        </button>
                      </div>
                      <!--end::Actions-->
                    </form>
                    <!--end::Form-->
                  </div>
                  <!--end::Modal body-->
                </div>
                <!--end::Modal content-->
              </div>
              <!--end::Modal dialog-->
            </div>
            <!--end::Modal - Add Blog Post-->
            <!--begin::Modal - Edit Blog Post-->
            <div class="modal fade" id="kt_modal_edit_content_blog_posts" tabindex="-1" aria-hidden="true">
              <!--begin::Modal dialog-->
              <div class="modal-dialog modal-dialog-centered mw-650px">
                <!--begin::Modal content-->
                <div class="modal-content">
                  <!--begin::Modal header-->
                  <div class="modal-header" id="kt_modal_edit_content_blog_posts_header">
                    <!--begin::Modal title-->
                    <h2 class="fw-bold">Edit Blog Post</h2>
                    <!--end::Modal title-->
                    <!--begin::Close-->
                    <div class="btn btn-icon btn-sm btn-active-icon-primary" data-kt-edit-content-blog-post-modal-action="close">
                      <i class="bi bi-x fs-2x"></i>
                    </div>
                    <!--end::Close-->
                  </div>
                  <!--end::Modal header-->
                  <!--begin::Modal body-->
                  <div class="modal-body px-5 my-7">
                    <!--begin::Form-->
                    <form id="kt_modal_edit_content_blog_posts_form" class="form" action="/api/settings/content/blog-posts" method="post">
                      <!--begin::Scroll-->
                      <div class="d-flex flex-column scroll-y px-5 px-lg-10" id="kt_modal_edit_content_blog_posts_scroll" data-kt-scroll="true" data-kt-scroll-activate="true" data-kt-scroll-max-height="auto" data-kt-scroll-dependencies="#kt_modal_edit_content_blog_posts_header" data-kt-scroll-wrappers="#kt_modal_edit_content_blog_posts_scroll" data-kt-scroll-offset="300px">
                        <!--begin::Input group-->
                        <div class="fv-row mb-10">
                          <!--begin::Label-->
                          <label class="form-label required">Thumbnail</label>
                          <!--end::Label-->
                          <!--begin::Input group-->
                          <div class="fv-row">
                            <!--begin::Preview Container-->
                            <div class="kt-file-uploader">
                              <!--begin::Message-->
                              <label class="kt-file-uploader-label d-flex flex-column flex-xl-row align-items-center align-items-xl-start text-center text-xl-start" for="kt_file_uploader_edit_content_blog_posts_thumbnail">
                                <i class="ki-duotone ki-file-up fs-3x text-primary"><span class="path1"></span><span class="path2"></span>
                                </i>
                                <!--begin::Info-->
                                <div class="ms-xl-4 mt-4 mt-xl-0">
                                  <h3 class="fs-5 fw-bold text-gray-900 mb-1">
                                    Upload thumbnail
                                  </h3>
                                  <span class="fs-7 fw-semibold text-gray-500">
                                    PNG, JPG, JPEG, GIF up to 10MB
                                  </span>
                                </div>
                                <!--end::Info-->
                              </label>
                              <!--end::Message-->
                              <!--begin::Input-->
                              <input type="file" name="thumbnail" id="kt_file_uploader_edit_content_blog_posts_thumbnail" accept=".png, .jpg, .jpeg" data-kt-file-uploader-max-size="10" hidden class="d-none" />
                              <!--begin::Error Message -->
                              <div class="invalid-feedback d-block"></div>
                              <!--end::Error Message-->
                            </div>
                            <!--end::Preview Container-->
                          </div>
                          <!--end::Input group-->
                        </div>
                        <!--end::Input group-->
