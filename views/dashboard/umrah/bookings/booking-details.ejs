<!DOCTYPE html>
<html lang="en">
<!--begin::Head-->
<%- include('../../partials/head', { title }) %>
<!--end::Head-->
<!--begin::Body-->

<body id="kt_app_body" data-kt-app-layout="light-sidebar" data-kt-app-header-fixed="true" data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true" data-kt-app-sidebar-push-header="true" data-kt-app-sidebar-push-toolbar="true" data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true" class="app-default">
  <!--begin::Theme mode setup on page load-->
  <%- include('../../partials/theme-mode.ejs') %>
  <!--end::Theme mode setup on page load-->
  <!--begin::App-->
  <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
    <!--begin::Page-->
    <div class="app-page flex-column flex-column-fluid" id="kt_app_page">
      <!--begin::Header-->
      <%- include('../../partials/header', { user }) %>
      <!--end::Header-->
      <!--begin::Wrapper-->
      <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
        <!--begin::Sidebar-->
        <%- include('../../partials/sidebar') %>
        <!--end::Sidebar-->
        <!--begin::Main-->
        <!--begin::Content wrapper-->
        <!--begin::Main-->
        <div class="app-main flex-column flex-row-fluid" id="kt_app_main">
          <!--begin::Content wrapper-->
          <div class="d-flex flex-column flex-column-fluid">
            <!--begin::Toolbar-->
            <div id="kt_app_toolbar" class="app-toolbar py-3 py-lg-6">
              <!--begin::Toolbar container-->
              <div id="kt_app_toolbar_container" class="app-container container-xxl d-flex flex-stack"></div>
              <!--end::Toolbar container-->
            </div>
            <!--end::Toolbar-->
            <!--begin::Content-->
            <div id="kt_app_content" class="app-content flex-column-fluid">
              <!--begin::Content container-->
              <div id="kt_app_content_container" class="app-container container-xxl">
                <!--begin::Layout-->
                <div class="d-flex flex-column flex-lg-row">
                  <!--begin::Content-->
                  <div class="flex-lg-row-fluid me-lg-15">
                    <!--begin::Booking Reference Card-->
                    <div class="card mb-6 mb-xl-9">
                      <!--begin::Card header-->
                      <div class="bg-light-red rounded-t fw-semibold text-t-600 reference-card-header">
                        <p class="fs-4 mb-0">
                          Booking Reference :
                          <span class="text-red"><%= umrahBooking.bookingRefId %></span>
                        </p>
                      </div>
                      <!--end::Card header-->
                      <!--begin::Card body-->
                      <div class="lh-lg d-grid grid-cols-11 justify-content-lg-between align-items-lg-center reference-card text-t-600">
                        <div class="d-grid grid-cols-3 reference-card-left">
                          <ul class="col-span-1 d-grid gap-5">
                            <li>Customer</li>
                            <li>Email</li>
                            <li>Phone</li>
                            <li>Booked</li>
                            <li>Status</li>
                          </ul>
                          <ul class="col-span-2 d-grid gap-5">
                            <li>: <%= umrahBooking?.customer.name %></li>
                            <li>: <%= umrahBooking?.customer.email %></li>
                            <li>: <%= umrahBooking?.customer.phone %></li>
                            <li>: <%= umrahBooking?.customer.phone %></li>
                            <li>
                              :
                              <span class="text-success" data-kt-umrah-booking-status="umrah-status"><%= umrahBooking?.status.charAt(0).toUpperCase() + umrahBooking?.status.slice(1) %></span>
                            </li>
                          </ul>
                        </div>
                        <div class="d-grid grid-cols-3 reference-card-right">
                          <ul class="col-span-1 d-grid gap-5">
                            <li>Booked On</li>
                            <li>Payment Type</li>
                            <li>Issue At</li>
                            <li>Invoice</li>
                            <li>Supplier</li>
                          </ul>
                          <ul class="col-span-2 d-grid gap-5">
                            <li>: <%= new Date(umrahBooking?.umrahPackage?.updatedAt).toLocaleString() %></li>
                            <li>: <%= umrahBooking?.invoiceDetails?.paymentType.charAt(0).toUpperCase() + umrahBooking?.invoiceDetails?.paymentType.slice(1) %></li>
                            <li>:
                              <!-- 18 February, 2024 - 22:00:22 -->
                            </li>
                            <li>: <%= umrahBooking?.invoiceDetails?.invoiceId %></li>
                            <li>:
                              <!-- Best Trip Travel -->
                            </li>
                          </ul>
                        </div>
                      </div>
                      <!--end::Card body-->
                    </div>
                    <!--end::Booking Reference Card-->
                    <!--begin::Booking Details Card-->
                    <div class="card mb-6 mb-xl-9">
                      <!--begin::Card body-->
                      <div class="details-card-content">
                        <image src="<%= umrahBooking?.umrahPackage?.thumbnail %>" alt="booking pic" class="details-card-img" />
                        <div class="details-card-text-content">
                          <div>
                            <h4 class="details-card-title">
