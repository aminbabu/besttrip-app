                          <div class="text-gray-600"><%= customer?.customerID %></div>
                          <!--end::Details item-->
                          <!--begin::Details item-->
                          <div class="fw-bold mt-5">Birthday</div>
                          <div class="text-gray-600"><%= customer?.dob %></div>
                          <!--end::Details item-->
                          <!--begin::Details item-->
                          <div class="fw-bold mt-5">Email Address</div>
                          <div class="text-gray-600"><%= customer?.email %></div>
